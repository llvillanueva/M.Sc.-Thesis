@article{JEONG2001165,
title = {Hyperdifferential Operators and Continuous Functions on Function Fields},
journal = {Journal of Number Theory},
volume = {89},
number = {1},
pages = {165-178},
year = {2001},
issn = {0022-314X},
doi = {https://doi.org/10.1006/jnth.2000.2629},
url = {https://www.sciencedirect.com/science/article/pii/S0022314X00926295},
author = {Sangtae Jeong},
abstract = {We prove that a sequence of hyperdifferential operators is an orthonormal basis of the space of continuous Fq-linear functions on Fq[[T]]. By Conrad's digit principle, the q-adic extensions of this sequence, called the digit derivatives, turn out to be an orthonormal basis of the whole space of continuous functions on Fq[[T]]. We then give the explicit derivation of the formula for digit derivative coefficients.}
}

